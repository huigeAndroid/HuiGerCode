<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LikeView">
        <attr name="likeViewCirRadius" format="dimension"/>
        <attr name="likeViewCirTime" format="integer"/>
        <attr name="likeViewDefaultColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="TabScrollView">
        <attr name="tabTitleSize" format="dimension"/>
        <attr name="tabTitleSelColor" format="color"/>
        <attr name="tabTitleUnSelColor" format="color"/>
        <attr name="tabTitlePadding" format="dimension"/>
        <attr name="tabTitleLineVisible" format="enum">
            <enum name="GONE" value="-1"/>
            <enum name="VISIBLE" value="1"/>
        </attr>
        <attr name="tabTitleLineBgColor" format="color"/>
    </declare-styleable>

</resources>